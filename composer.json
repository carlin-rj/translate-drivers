{
    "name": "carlin/translate-drives",
    "type": "library",
    "license": "Apache-2.0",
    "description": "Baidu, Alibaba Cloud Translate, and Google Translate driver package, ready to use out of the box",
    "keywords": [
        "php",
        "translate",
        "baidu",
        "alibaba cloud",
        "google translate",
        "free google translate",
        "translate drives"
    ],
    "autoload": {
        "psr-4": {
            "Carlin\\TranslateDrives\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carlin\\TranslateDrives\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "wanghaojie",
            "email": "814425737@qq.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "stichoza/google-translate-php": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "guzzlehttp/guzzle": "^7.0"
    }
}
